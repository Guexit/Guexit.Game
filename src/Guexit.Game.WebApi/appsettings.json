{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ServiceBus": {
    "Enabled": true
  },
  "AllowedHosts": "*",
  "ImageGenerationRecurrentTask": {
    "Enabled": true,
    "PeriodInSeconds": 10,
    "TargetAvailableImagePoolSize": 200
  },
  "Database": {
    "MigrateOnStartup": false
  }
}
