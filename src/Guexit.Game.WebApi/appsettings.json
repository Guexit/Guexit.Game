{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ImageGenerationRecurrentTask": {
    "Enabled": true,
    "PeriodInSeconds": 60,
    "TargetAvailableImagePoolSize": 2000
  },
  "Database": {
    "MigrateOnStartup": false
  }
}
